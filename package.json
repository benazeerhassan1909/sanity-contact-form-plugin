{
  "name": "@multidots/sanity-plugin-contact-form",
  "version": "1.0.2",
  "description": "Sanity plugin for contact form creation",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "multidots",
  "sideEffects": false,
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict node watch-and-push.js",
    "dev": "concurrently \"npm run build --watch\" \"npm run watch\""
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "nodemailer": "^7.0.3",
    "react-google-recaptcha": "^3.1.0"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^7.2.3",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/next": "^8.0.7",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.6",
    "@types/react-google-recaptcha": "^2.1.9",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanity": "^3.90.0",
    "styled-components": "^6.1.18",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^19",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/dist/**"
    ]
  }
}
